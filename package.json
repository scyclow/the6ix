{
  "author": "Steve Pikelny",
  "license": "MIT",
  "name": "es6-skel",
  "version": "0.1.0",
  "description": "Build skeleton for ES6 project",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/scyclow/es6-skel"
  },
  "scripts": {
    "build:webpack": "NODE_ENV='production' webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "clean": "rimraf build",
    "serve-static": "node serveStatic.js",
    "watch": "npm run clean && webpack && webpack --watch",
    "w": "npm run clean && NODE_ENV='development' node index.js",
    "unit": "karma start"
  },
  "dependencies": {
    "babel-polyfill": "^6.6.1",
    "babel-runtime": "^6.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "connect": "^3.4.1",
    "eslint": "^1.5.0",
    "eslint-config-airbnb": "0.0.8",
    "eslint-loader": "^1.0.0",
    "express": "^4.13.4",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.2",
    "pretty-error": "^2.0.0",
    "rimraf": "^2.4.3",
    "serve-static": "^1.10.2",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
